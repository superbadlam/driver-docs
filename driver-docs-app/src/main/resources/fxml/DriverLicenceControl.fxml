<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="10.0"
         type="javafx.scene.layout.VBox" xmlns="http://javafx.com/javafx/10.0.2-internal">
    <VBox>
        <TextField fx:id="txtLicSeries" promptText="серия"/>
        <TextField fx:id="txtLicNumber" promptText="номер"/>
        <DatePicker fx:id="dtLicStart" promptText="дата начала"/>
        <DatePicker fx:id="dtLicEnd" promptText="дата окончания"/>
    </VBox>
    <HBox alignment="CENTER_RIGHT">
        <Button fx:id="btnLicApply" mnemonicParsing="false" text="применить"/>
        <Button fx:id="btnLicDelete" layoutX="654.0" layoutY="10.0" mnemonicParsing="false" text="удалить"/>
    </HBox>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</fx:root>
