<?xml version="1.0" encoding="UTF-8"?>

<!--fx:controller="fxml.DriverDocuments"-->

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" alignment="TOP_CENTER" spacing="5.0" type="javafx.scene.layout.VBox"
         xmlns="http://javafx.com/javafx/10.0.2-internal">
    <VBox spacing="5.0">
        <VBox.margin>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </VBox.margin>
        <TextField fx:id="txtLicSeries" promptText="серия"/>
        <TextField fx:id="txtLicNumber" promptText="номер"/>
        <DatePicker fx:id="dtLicStart" promptText="дата начала"/>
    </VBox>
    <HBox alignment="CENTER_RIGHT" spacing="5.0">
        <Button fx:id="btnLicApply" mnemonicParsing="false" text="применить"/>
        <Button fx:id="btnLicDelete" layoutX="654.0" layoutY="10.0" mnemonicParsing="false" text="удалить"/>
        <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
        </padding>
    </HBox>
    <VBox.margin>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </VBox.margin>
</fx:root>
